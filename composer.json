{
    "name": "cwp/cwp-pdfexport",
    "description": "Add PDF export capability for pages in CWP",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "cwp"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.3 || ^8.0",
        "silverstripe/framework": "^4.10",
        "cwp/cwp": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": [],
    "autoload": {
        "psr-4": {
            "CWP\\PDFExport\\": "src/",
            "CWP\\PDFExport\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}